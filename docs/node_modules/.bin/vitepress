#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/FKaique/Documentos/projetos/Games/PantsLife/PantsLife/node_modules/.pnpm/vitepress@2.0.0-alpha.16_postcss@8.5.6_typescript@5.9.3_yaml@2.8.2/node_modules/vitepress/bin/node_modules:/home/FKaique/Documentos/projetos/Games/PantsLife/PantsLife/node_modules/.pnpm/vitepress@2.0.0-alpha.16_postcss@8.5.6_typescript@5.9.3_yaml@2.8.2/node_modules/vitepress/node_modules:/home/FKaique/Documentos/projetos/Games/PantsLife/PantsLife/node_modules/.pnpm/vitepress@2.0.0-alpha.16_postcss@8.5.6_typescript@5.9.3_yaml@2.8.2/node_modules:/home/FKaique/Documentos/projetos/Games/PantsLife/PantsLife/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/FKaique/Documentos/projetos/Games/PantsLife/PantsLife/node_modules/.pnpm/vitepress@2.0.0-alpha.16_postcss@8.5.6_typescript@5.9.3_yaml@2.8.2/node_modules/vitepress/bin/node_modules:/home/FKaique/Documentos/projetos/Games/PantsLife/PantsLife/node_modules/.pnpm/vitepress@2.0.0-alpha.16_postcss@8.5.6_typescript@5.9.3_yaml@2.8.2/node_modules/vitepress/node_modules:/home/FKaique/Documentos/projetos/Games/PantsLife/PantsLife/node_modules/.pnpm/vitepress@2.0.0-alpha.16_postcss@8.5.6_typescript@5.9.3_yaml@2.8.2/node_modules:/home/FKaique/Documentos/projetos/Games/PantsLife/PantsLife/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
